{
  "name": "gemai",
  "version": "0.0.10",
  "description": "AI Chatbot in a terminal environment.",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": "./dist/index.js",
  "main": "./dist/index.js",
  "bin": {
    "gemai": "./dist/index.js"
  },
  "preferGlobal": true,
  "license": "MIT",
  "author": {
    "name": "sujjeee",
    "url": "https://twitter.com/sujjeeee"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sujjeee/gemai.git"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "gemini",
    "google gemini",
    "ai",
    "gemai",
    "chatbot",
    "cli",
    "sujjeee"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist",
    "start": "node dist/index.js ",
    "lint": "biome lint .",
    "lint:fix": "biome lint --apply-unsafe .",
    "format": "biome format .",
    "format:fix": "biome format --write .",
    "code-check": "biome check .",
    "code-check:fix": "biome check --apply-unsafe .",
    "biome:ci": "biome ci .",
    "pub:release": "pnpm build && pnpm publish --access public"
  },
  "dependencies": {
    "@google/generative-ai": "^0.1.3",
    "@langchain/community": "^0.0.15",
    "@langchain/core": "^0.1.63",
    "@langchain/google-genai": "^0.0.6",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "configstore": "^6.0.0",
    "d3-dsv": "^2.0.0",
    "faiss-node": "^0.5.1",
    "fs-extra": "^11.2.0",
    "html-to-text": "^9.0.5",
    "jsdom": "^23.2.0",
    "json-colorizer": "^2.2.2",
    "langchain": "^0.1.37",
    "nanoid": "^5.0.7",
    "node-fetch": "^3.3.2",
    "ora": "^7.0.1",
    "package-json": "^8.1.1",
    "pdf-parse": "^1.1.1",
    "prompts": "^2.4.2",
    "qrcode": "^1.5.4",
    "rimraf": "^5.0.10",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "1.4.1",
    "@types/configstore": "^6.0.2",
    "@types/fs-extra": "^11.0.4",
    "@types/html-to-text": "^9.0.4",
    "@types/node": "^20.16.1",
    "@types/prompts": "^2.4.9",
    "@types/qrcode": "^1.5.5",
    "ts-node": "^10.9.2",
    "tsup": "^8.2.4",
    "type-fest": "^4.25.0",
    "typescript": "^5.5.4"
  }
}
